{
    "name": "@guieen/node-starter",
    "version": "0.1.27",
    "description": "A simple command-line utility for starting TS/JS project ðŸŒˆ",
    "main": "./dist/starter.js",
    "types": "./dist/starter.d.ts",
    "scripts": {
        "build": "rm -rf dist && tsc --strictNullChecks",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json"
    },
    "author": "Seungyong Kwak <guieenoutis@gmail.com>",
    "license": "MIT",
    "bin": {
        "node-starter": "./bin.js"
    },
    "repository": "GUIEEN/node-starter",
    "preferGlobal": true,
    "keywords": [
        "starter",
        "node-starter",
        "starterkit",
        "simple",
        "express",
        "backend",
        "frontend"
    ],
    "dependencies": {
        "colorlog-css": "^1.0.11",
        "commander": "^2.19.0",
        "fs-extra": "^7.0.1",
        "mkdirp": "^0.5.1",
        "rimraf": "^2.6.3"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.5",
        "@types/mkdirp": "^0.5.2",
        "@types/node": "^11.11.6",
        "@types/rimraf": "^2.0.2",
        "prettier": "^1.16.4",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^8.0.1",
        "typescript": "^3.3.4000"
    }
}
